   "source": [
    "# Find the factorial of an n!"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "id": "122d2e1c",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter a number: 6\n",
      "The factorial is 720\n"
     ]
    }
   ],
   "source": [
    "n = int(input(\"Enter a number: \"))\n",
    "f = 1\n",
    "if n<0:\n",
    "   print(\"Negative numbers not acceptable. \")\n",
    "elif n==0:\n",
    "   print(\"The factorial of 0 is 1\")\n",
    "else:\n",
    "   for i in range(1,n+1):\n",
    "       f = f*i\n",
    "   print(\"The factorial is\",f)"
   ]
